# Application
NODE_ENV=development
PORT=3000
API_URL=http://localhost:3000
FRONTEND_URL=http://localhost:3001

# Database
DATABASE_URL=postgresql://postgres:password@localhost:5432/greenai
REDIS_URL=redis://localhost:6379

# Authentication
JWT_SECRET=your-jwt-secret
JWT_EXPIRY=24h

# Blockchain
SOLANA_NETWORK=devnet
SOLANA_RPC_URL=https://api.devnet.solana.com
PUMP_FUN_API_KEY=your-pump-fun-api-key

# AI Services
OPENAI_API_KEY=your-openai-api-key
STABLE_DIFFUSION_API_KEY=your-stable-diffusion-api-key

# Storage
IPFS_NODE=your-ipfs-node
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=us-east-1
AWS_BUCKET_NAME=greenai-storage

# External Services
TWITTER_API_KEY=your-twitter-api-key
TWITTER_API_SECRET=your-twitter-api-secret
TWITTER_ACCESS_TOKEN=your-twitter-access-token
TWITTER_ACCESS_SECRET=your-twitter-access-secret

# Monitoring
SENTRY_DSN=your-sentry-dsn
NEW_RELIC_LICENSE_KEY=your-new-relic-key

# Feature Flags
ENABLE_AI_MEME_GENERATION=true
ENABLE_HARDWARE_INTEGRATION=true
ENABLE_SOCIAL_SHARING=true 